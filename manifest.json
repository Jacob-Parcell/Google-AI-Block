{
    "name": "AI Blocker",
    "description": "Removes AI Overview from Google Search",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_icon": "blocker_thumbnail.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/ai_blocker.js"],
          "matches": ["https://www.google.com/search?q=*"]
        }
      ]
}
