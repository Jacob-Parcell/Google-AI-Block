{
    "name": "AI Blocker",
    "description": "Removes AI Overview from Google Search",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
        {
          "js": ["scripts/ai_blocker.js"],
          "matches": ["https://www.google.com/search?q=*"]
        }
      ]
}
